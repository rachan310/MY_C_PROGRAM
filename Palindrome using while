
#include <stdio.h>
int main() {
    int num,org,rem,rev=0;
    printf("Enter the Number num\n");
    scanf("%d",&num);
    org = num;
    while(num!=0){
        rem=num%10;
        rev=rev*10+rem;
        num=num/10;
    }
    if(org==rev){
        printf("palindrome");
    }else{
        printf("not palindrome");
    }
}
