#include<stdio.h>
int main(){
    float marks;
    printf("Enter the marks of the student\n");
    scanf("%f",&marks);
    if(marks>=90){
        printf("Grade A");
    }
    else if(marks>=75){
        printf("Grade B");
    }
    else if(marks>=60){
        printf("Grade C");
    }
    else if(marks>=50){
        printf("Grade D");
    }
        else{
            printf("Grade F");
        }
}
